<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="css/admin.css" rel="stylesheet" />
    <title>Document</title>
</head>
<body>
    <!-- Hello admin! -->
    <h1>This is, <%= user.userName %></h1>
    <h3><%= user.email %></h3>
    <h3>Grade:<%= user.grade %></h3>
    <ul>
        <% allUsers.forEach( user => { %>
                <li class='user' data-id='<%=user._id%>'>
                   <!-- <a href="/todos?id=<%=user._id%>"> <span ><%= user.email %></span> </a> -->

                   <!-- this path is bringing me to each individual user's page -->
                   <a href="/showoneuser?id=<%=user._id%>"> <span ><%= user.email %></span> </a>
                    <span > is a/an <%= user.userType %></span>
                    <!-- <span class='del'> Delete </span> -->
                </li>
        <% }) %>    
        </ul>
        <!-- <a href="/"><h3>Home <%= user.userName %>'s home page</h3></a> -->
        <a href="/"><h3>Home</h3></a>
        <a href="/logout"><h3>Logout</h3></a>
        
</body>
</html>